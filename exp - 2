#include <stdio.h>

int main() {
    int a[10], i;
    printf("Enter the values:\n");
    
    for (i = 0; i < 10; i++) {
        scanf("%d", &a[i]);
    }
    
    printf("Classifying numbers as even or odd:\n");
    
    for (i = 0; i < 10; i++) {
        if (a[i] % 2 == 0) {
            printf("%d - even\n", a[i]);
        } else {
            printf("%d - odd\n", a[i]);
        }
    }
    
    return 0;
}
